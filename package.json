{
  "name": "cloud-cost-cli",
  "version": "0.7.0",
  "description": "Optimize your cloud spend in seconds",
  "main": "dist/index.js",
  "bin": {
    "cloud-cost-cli": "dist/bin/cloud-cost-cli.js"
  },
  "scripts": {
    "dev": "tsx bin/cloud-cost-cli.ts",
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "tsc",
    "build:frontend": "cd dashboard && npm install && npm run build",
    "prepublishOnly": "npm run build",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "cloud",
    "aws",
    "azure",
    "gcp",
    "cost",
    "optimization",
    "cli",
    "finops",
    "cost-optimization",
    "aws-cli",
    "devops",
    "cloud-cost",
    "google-cloud"
  ],
  "author": "Phuong Vu <vuhuuphuong@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/vuhp/cloud-cost-cli.git"
  },
  "bugs": {
    "url": "https://github.com/vuhp/cloud-cost-cli/issues"
  },
  "homepage": "https://github.com/vuhp/cloud-cost-cli#readme",
  "files": [
    "dist",
    "dashboard/dist",
    "README.md",
    "LICENSE",
    "docs"
  ],
  "license": "MIT",
